{
  "name": "prisma",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "storybook": "storybook dev --quiet -p 58413",
    "storybook:ci:build": "STORYBOOK_CI=true NODE_OPTIONS='--openssl-legacy-provider --max-old-space-size=8192' storybook build --quiet --output-dir storybook",
    "storybook:e2e:build": "STORYBOOK_NOT_MINIFIED=true storybook build --quiet --output-dir storybook && pnpm live-server --port=58413 storybook --no-browser",
    "storybook:e2e": "start-server-and-test storybook:e2e:build http://127.0.0.1:58413 'cypress run --e2e --browser chrome --headless --config-file cypress.config.ts'",
    "storybook:e2e:open": "start-server-and-test 'storybook:e2e:build' http://127.0.0.1:58413 'cypress open --e2e --browser chrome --config-file cypress.config.ts'",
    "build-storybook": "storybook build",
    "test-storybook:run": "DEBUG_PRINT_LIMIT=0 test-storybook --index-json",
    "test-storybook:build": "cross-env STORYBOOK_NOT_MINIFIED=true NODE_OPTIONS='--max-old-space-size=8192' storybook build --quiet --output-dir storybook",
    "test-storybook:server": "pnpm live-server --port=58414 storybook --no-browser",
    "test-storybook:build-and-run": "pnpm test-storybook:build && start-server-and-test 'pnpm test-storybook:server' http-get://127.0.0.1:58414 'cross-env TARGET_URL=http://127.0.0.1:58414 pnpm test-storybook:run'",
    "test-storybook:generate-images": "cross-env GENERATE_IMAGES_LOCALLY=true pnpm test-storybook:build && start-server-and-test 'pnpm test-storybook:server' http-get://127.0.0.1:58414 'cross-env TARGET_URL=http://127.0.0.1:58414 pnpm test-storybook:run -u'",
    "start-playwright-server": "docker run -p 3000:3000 --rm --init -it mcr.microsoft.com/playwright:v1.45.1-jammy /bin/sh -c \"cd /home/pwuser && npx -y playwright@1.45.1 run-server --port 3000\"",
    "start-playwright-server-linux": "docker run -p 3000:3000 --add-host=host.docker.internal:host-gateway --rm --init -it mcr.microsoft.com/playwright:v1.45.1-jammy /bin/sh -c \"cd /home/pwuser && npx -y playwright@1.45.1 run-server --port 3000\""
  },
  "dependencies": {
    "@types/react": "^18.2.37",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "8.2.5",
    "@storybook/addon-backgrounds": "8.2.5",
    "@storybook/addon-docs": "8.2.5",
    "@storybook/addon-essentials": "8.2.5",
    "@storybook/addon-interactions": "8.2.5",
    "@storybook/addon-links": "8.2.5",
    "@storybook/addon-mdx-gfm": "8.2.5",
    "@storybook/blocks": "8.2.5",
    "@storybook/components": "8.2.5",
    "@storybook/manager-api": "8.2.5",
    "@storybook/preview-api": "8.2.5",
    "@storybook/react": "8.2.5",
    "@storybook/react-vite": "8.2.5",
    "@storybook/test": "8.2.5",
    "@storybook/test-runner": "0.19.1",
    "@storybook/theming": "8.2.5",
    "@storybook/types": "8.2.5",
    "@styled/typescript-styled-plugin": "^1.0.0",
    "@swc/jest": "0.2.36",
    "@vitejs/plugin-react": "4.3.1",
    "@vitejs/plugin-react-swc": "3.7.0",
    "concurrently": "^7.6.0",
    "core-js": "^3.30.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "globby": "^11.0.0",
    "jest-image-snapshot": "^6.4.0",
    "live-server": "^1.2.2",
    "playwright": "^1.45.3",
    "prettier": "3.3.3",
    "remark-gfm": "4.0.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "start-server-and-test": "^1.14.0",
    "storybook": "8.2.5",
    "typescript": "5.5.4",
    "vite": "5.3.4",
    "vite-node": "2.0.4"
  },
  "engines": {
    "node": ">=20.0.3 <22.0.0",
    "pnpm": ">=9.0.0"
  }
}
